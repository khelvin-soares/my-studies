algoritmo "Desafio 011"
var
    A, B, C: Inteiro
    Delta: Real
inicio
    Escreva("Qual o valor de A? ")
    Leia(A)
    Escreva("Qual o valor de B? ")
    Leia(B)
    Escreva("Qual o valor de C? ")
    Leia(C)
    
    Delta <- (B^2)-4*A*C
    Escreval("")
    Escreva("O valor de DELTA:", Delta)
fimalgoritmo